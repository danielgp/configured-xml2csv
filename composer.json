{
    "name": "danielgp/configured-xml2csv",
    "description": "Transforming XML input into CSV based on few configuration rules",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Popiniuc",
            "email": "danielpopiniuc@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0 || ^7.1"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "*",
        "frenck/php-compatibility": "*",
        "phpunit/phpunit": "~5.7",
        "phpunit/php-code-coverage": ">=4.0"
    },
    "autoload": {
        "classmap": [
            "source/ConfiguredXmlToCsv.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "archive": {
        "exclude": ["/nbproject", "/vendor", "/tests"]
    }
}
